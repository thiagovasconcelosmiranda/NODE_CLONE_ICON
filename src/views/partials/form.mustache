                   <option vlaue="Outros">Outros</option>
                 </select>
              </div>
              <div class="group-input-3">
                <select class="
                    {{#error.item1}}
                       error-input
                    {{/error.item1}}
                " type="text" name="item3" >
                  <option value="{{item3}}">você é surdo?</option>
                  <option value="Sim">Sim</option>
